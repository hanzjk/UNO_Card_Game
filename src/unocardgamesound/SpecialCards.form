    <Component class="javax.swing.JLabel" name="WILD">
      <Properties>
        <Property name="font" type="java.awt.Font" editor="org.netbeans.beaninfo.editors.FontEditor">
          <Font name="Tahoma" size="18" style="0"/>
        </Property>
        <Property name="text" type="java.lang.String" value="&lt;html&gt;&#xa;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;strong&gt;WILD&lt;/strong&gt;&lt;/p&gt;&#xa;The player who plays this card can choose to change the current color or to continue with the same color.&#xa;&lt;br&gt;If it is the first card of the stock, then the first player must choose the starting color.&lt;/br&gt;&#xa;&lt;br&gt;This card can be played on any other card.&lt;/br&gt;&#xa;&lt;/html&gt;"/>
      </Properties>
      <Constraints>
